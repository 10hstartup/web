<%- include('blocks/header') %>
    <div class="container mt-2">
      <h1>Your Profile</h1>
      <form id="submit">
        <div class="form-group">
          <label for="name">Username</label>
          <input type="text" class="form-control" id="name">
        </div>
        <div class="form-group">
          <label for="id">ID</label>
          <input type="text" class="form-control" id="id" disabled>
        </div>
        <div class="form-group">
          <label for="rep">Reputation</label>
          <input type="text" class="form-control" id="rep" placeholder="0" disabled>
        </div>
        <div class="form-group">
          <label for="email">Email</label>
          <input type="email" class="form-control" id="email">
        </div>
        <div class="form-group">
          <label for="phone">Phone</label>
          <input type="phone" class="form-control" id="phone">
        </div>
        <div class="form-group">
          <label for="location">Location</label>
          <input type="location" class="form-control" id="location">
        </div>
        <div class="form-group">
          <label for="password">Password</label>
          <input type="text" class="form-control" id="password">
        </div>
        <div class="g-recaptcha" data-sitekey="6LepN-MUAAAAAIaZKV-BSSEm-krAwdDo3jzZP53t" data-callback="enable"></div><br>
        <button type="submit" class="btn btn-primary" id="save" disabled><i class="fas fa-save" style="color: #fff !important"></i> Save Profile</button>
      </form>
    </div>
  </div><br>
  <footer class="text-center">
    <div class="container">
      <div class="row">
        <div class="col-12">
          <p>Copyright &copy; 2020 COVID Heroes. All rights reserved.</p>
        </div>
      </div>
    </div>
  </footer>
  <script src="./user.js" defer></script>
</body>
</html>
