<%- include('blocks/header') %>
    <div class="container"></div>
    <div class="container mt-2">
      <h1 id="reqid"></h1>
      <form id="req">
        <div class="form-group">
          <label for="item">Item</label>
          <input type="text" class="form-control" id="item" readonly>
        </div>
        <div class="form-group">
          <label for="author">Author</label>
          <input type="text" class="form-control" id="author" readonly>
        </div>
        <div class="form-group">
          <label for="date">Date</label>
          <input type="email" class="form-control" id="date" readonly>
        </div>
        <div class="form-group">
          <label for="quantity">Quantity</label>
          <input type="phone" class="form-control" id="quantity" readonly>
        </div>
        <button class="btn btn-primary" id="fulfill" id="submit">Fulfill</button>
      </form>
    </div>
  </div><br>
  <footer class="text-center">
    <div class="container">
      <div class="row">
        <div class="col-12">
          <p>&copy; 2019-present CoronaExchange. All rights reserved.</p>
        </div>
      </div>
    </div>
  </footer>
  <script src="/request.js" defer></script>
</body>
</html>
